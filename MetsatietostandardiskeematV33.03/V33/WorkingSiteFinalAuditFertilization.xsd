					<xs:documentation xml:lang="fi">Lentopaikka (nousu/lasku) ohjeistuksen mukainen</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AirdromeAsInstructedText" type="bdt:String200Type" minOccurs="0">
