    </xs:simpleType>

    <xs:complexType name="FinancingActApplicationStandsType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Kemera-haun kuviot.</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en">Stands data in application</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="FinancingActApplicationStand" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="FinancingActApplicationStandType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Kemera-haun kuvio on yhtenäinen alue (Polygon, ei MultiPolygon). Kemera-kuviolla ei tarvitse olla yhteyttä metsikkökuviointiin mutta Kemera-kuvion tulee sijaita kokonaisuudessaan saman metsänomistajan kiinteistöillä. Kuvionumerointi tulee olla yksilöivä kiinteistön sisällä eli Kemera-kuviolla tulee olla kiinteistöittäin (LocationEstate) yksilöivä kuvionumerointi (StandNumber ja StandNumberExtension -yhdistelmä). Hakemuksen lähettäjän osalta kuvioviitteen (StandReference) tulee olla yksilöivä.</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en">Stand data in application</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="StandNumber" minOccurs="1" maxOccurs="1"/>
            <xs:element ref="StandNumberExtension" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="StandReference" minOccurs="1" maxOccurs="1"/>
            <xs:element ref="Area" minOccurs="1" maxOccurs="1"/>
            <xs:element ref="MainGroup" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="SubGroup" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="FertilityClass" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="SoilType" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="MeanHeight" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="DiameterClass" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="CostTypeAndCompletedWorkApplication" minOccurs="1" maxOccurs="1"/>
            <xs:element ref="StandExtraInfo" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="gml:polygonProperty" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="fi">Kemera hakemuksen kuvion aluegeometria sisäalueineen.</xs:documentation>
                    <xs:documentation xml:lang="sv"/>
                    <xs:documentation xml:lang="en">Polygon geometry of stand of financing act application with interior polygons.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="LocationEstate" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="fi">Kuvion sijaintikiinteistöksi merkitään se kiinteistö, jonka alueella suurin osa kuvion pinta-alasta sijaitsee. Yksittäisen kuvion suositellaan sijaitsevan samalla kiinteistöllä. Mikäli kuitenkin esim. pinta-alavaatimuksen täyttymiseksi on tarpeen kuvioinnissa ylittää kiinteistön raja (saman omistajan vierekkäiset kiinteistöt) voidaan poikkeustilanteissa kuviointi tehdä useamman kiinteistön alueelle. Id-viittaus RealEstate.id -attribuuttiin.</xs:documentation>
                    <xs:documentation xml:lang="sv"/>
                    <xs:documentation xml:lang="en"/>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="id" type="co:IdStringType">
            <xs:annotation>
                <xs:documentation xml:lang="fi">Kuvion id</xs:documentation>
                <xs:documentation xml:lang="sv"/>
                <xs:documentation xml:lang="en">Stand id</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="FinancingActApplicationOtherSubjectsType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Muiden kuin aluemaisten kohteiden tiedot hakemuksella</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="CostTypeAndCompletedWorkApplicationRealEstate" minOccurs="1"
                maxOccurs="1"/>
            <xs:element ref="FinancingActApplicationGeometries" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="FinancingActApplicationGeometriesType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Viiva- ja pistemäisten kohteiden tiedot hakemuksella</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en">Lines and points data in application</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="FinancingActApplicationGeometry" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="FinancingActCompletionStandsType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Kuvioiden tiedot toteutusilmoituksella</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en">Stands data in completion</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="FinancingActCompletionStand" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="FinancingActCompletionStandType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Kemera-haun kuvio on yhtenäinen alue (Polygon, ei MultiPolygon). Kemera-kuviolla ei tarvitse olla yhteyttä metsikkökuviointiin mutta Kemera-kuvion tulee sijaita kokonaisuudessaan saman metsänomistajan kiinteistöillä. Kuvionumerointi tulee olla yksilöivä kiinteistön sisällä eli Kemera-kuviolla tulee olla kiinteistöittäin (LocationEstate) yksilöivä kuvionumerointi (StandNumber ja StandNumberExtension -yhdistelmä). Hakemuksen lähettäjän osalta kuvioviitteen (StandReference) tulee olla yksilöivä. Työlajeittain pakollisia tietoja ovat seuraavat kuviotiedot xml-skeemassa esitettyjen pakollisuuksien lisäksi. Taimikon varhaishoito: 1. jäävän puuston määrä (runkoluku), 2. keskipituus, 3. Poistuman suuruusluokka tai poistuman tarkka runkoluku; Nuoren metsän hoito: 1. jäävän puuston määrä (runkoluku), 2. keskiläpimitta, 3. Jäävän puuston pituusluokka tai keskipituus, 4. Poistuman suuruusluokka tai poistuman tarkka runkoluku; Juurikäävän torjunta: 1. Torjunta-alueen pinta-ala, 2. ajankohta, jolloin työ on tehty.</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en">Stand data in completion</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="StandNumber" minOccurs="1" maxOccurs="1"/>
            <xs:element ref="StandNumberExtension" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="StandReference" minOccurs="1" maxOccurs="1"/>
            <xs:element ref="Area" minOccurs="1" maxOccurs="1"/>
            <xs:element ref="MainGroup" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="SubGroup" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="FertilityClass" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="SoilType" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="MainTreeSpecies" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="fi">Jäävän puuston pääpuulaji</xs:documentation>
                    <xs:documentation xml:lang="en"/>
                    <xs:documentation xml:lang="sv"/>
                </xs:annotation>
            </xs:element>
            <xs:element ref="StemCount" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="fi">Jäävän puuston runkoluku, kpl/ha</xs:documentation>
                    <xs:documentation xml:lang="en"/>
                    <xs:documentation xml:lang="sv"/>
                </xs:annotation>
            </xs:element>
            <xs:element ref="HeightClass" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="fi">Jäävän puuston pituusluokka</xs:documentation>
                    <xs:documentation xml:lang="sv"/>
                    <xs:documentation xml:lang="en"/>
                </xs:annotation>
            </xs:element>
            <xs:element ref="MeanHeight" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="fi">Jäävän puuston keskipituus, m</xs:documentation>
                    <xs:documentation xml:lang="en"/>
                    <xs:documentation xml:lang="sv"/>
                </xs:annotation>
            </xs:element>
            <xs:element ref="DiameterClass" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="fi">Jäävän puuston läpimittaluokka (pohjapinta-alalla painotettu keskiläpimitta)</xs:documentation>
                    <xs:documentation xml:lang="sv"/>
                    <xs:documentation xml:lang="en"/>
                </xs:annotation>
            </xs:element>
            <xs:element ref="MeanDiameter" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="fi">Jäävän puuston keskiläpimitta, cm (pohjapinta-alalla painotettu keskiläpimitta)</xs:documentation>
                    <xs:documentation xml:lang="en"/>
                    <xs:documentation xml:lang="sv"/>
                </xs:annotation>
            </xs:element>
            <xs:element ref="RemovalClass" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="CuttingStemCount" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="SmallWoodRemovalClass" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="SmallWoodRemovalVolume" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="CostTypeAndCompletedWorkCompletion" minOccurs="1" maxOccurs="1"/>
            <xs:element ref="StandExtraInfo" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="gml:polygonProperty" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="fi">Kemera toteutusilmoituksen kuvion aluegeometria sisäalueineen.</xs:documentation>
                    <xs:documentation xml:lang="sv"/>
                    <xs:documentation xml:lang="en">Polygon geometry of stand of financing act completion with interior polygons.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="LocationEstate" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="fi">Kuvion sijaintikiinteistöksi merkitään se kiinteistö, jonka alueella suurin osa kuvion pinta-alasta sijaitsee. Yksittäisen kuvion suositellaan sijaitsevan samalla kiinteistöllä. Mikäli kuitenkin esim. pinta-alavaatimuksen täyttymiseksi on tarpeen kuvioinnissa ylittää kiinteistön raja (saman omistajan vierekkäiset kiinteistöt) voidaan poikkeustilanteissa kuviointi tehdä useamman kiinteistön alueelle. Id-viittaus RealEstate.id -attribuuttiin.</xs:documentation>
                    <xs:documentation xml:lang="sv"/>
                    <xs:documentation xml:lang="en"/>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="id" type="co:IdStringType">
            <xs:annotation>
                <xs:documentation xml:lang="fi">Kuvion id</xs:documentation>
                <xs:documentation xml:lang="sv"/>
                <xs:documentation xml:lang="en">Stand id</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="FinancingActCompletionOtherSubjectsType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Muiden kuin aluemaisten kohteiden tiedot toteutusilmoituksella</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="CostTypeAndCompletedWorkCompletionRealEstate" minOccurs="1"
                maxOccurs="1"/>
            <xs:element ref="FinancingActCompletionGeometries" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="FinancingActCompletionGeometriesType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Viiva- ja pistemäisten kohteiden tiedot toteutusilmoituksella</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en">Lines and points data in completion</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="FinancingActCompletionGeometry" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="FinancingActGeometryType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Yksittäisen viiva- tai pistemäisen kohteen tiedot</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en">Single line or point data</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="FeatureCode" minOccurs="1" maxOccurs="1"/>
            <xs:element ref="FeatureInfo" minOccurs="1" maxOccurs="1"/>
            <xs:group ref="gdt:PointAndLineGeometriesGroup"/>
        </xs:sequence>
        <xs:attribute name="id" type="co:IdStringType">
            <xs:annotation>
                <xs:documentation xml:lang="fi">Kohteen id</xs:documentation>
                <xs:documentation xml:lang="sv"/>
                <xs:documentation xml:lang="en">Geometry id</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:simpleType name="CuttingStemCountType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Poistuman runkoluku. Runkolukuelementin määrittely.</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
        <xs:restriction base="xs:positiveInteger">
            <xs:minInclusive value="1"/>
            <xs:maxInclusive value="99999"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="AmountType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Toteutetun tai toteutetavan työn määrä.</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
        <xs:restriction base="xs:decimal">
            <xs:totalDigits value="7"/>
            <xs:fractionDigits value="1" fixed="true"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="UnitPriceType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Yksikköhinta. Ykshintaelementin määrittely.</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
        <xs:restriction base="xs:decimal">
            <xs:totalDigits value="9"/>
            <xs:fractionDigits value="3" fixed="true"/>
            <xs:minInclusive value="0"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="CostTypeType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Kustannuslaji</xs:documentation>
            <xs:documentation xml:lang="sv"/>
            <xs:documentation xml:lang="en"/>
        </xs:annotation>
        <xs:sequence>
