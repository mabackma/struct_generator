			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>	
    <xs:element name="AdvertisementDating" type="VirtaAdvertisementDatingType"/>
    <xs:element name="AreaAndMapEvaluation" type="sp:VirtaEvaluationType">
