<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://standardit.tapio.fi/schemas/virta/inspectiondata"
	xmlns="http://standardit.tapio.fi/schemas/virta/inspectiondata" 	
