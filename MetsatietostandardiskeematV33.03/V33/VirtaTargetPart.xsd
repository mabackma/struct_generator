<xs:schema
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://standardit.tapio.fi/schemas/virta/targetpart"
	xmlns="http://standardit.tapio.fi/schemas/virta/targetpart" 
	xmlns:co="http://standardit.tapio.fi/schemas/forestData/common"	
	xmlns:sb="http://standardit.tapio.fi/schemas/forestData/standbase" 
	xmlns:sp="http://standardit.tapio.fi/schemas/virta/sampleplot" 
	xmlns:sfb="http://standardit.tapio.fi/schemas/forestData/specialFeatureBase" 
	xmlns:op="http://standardit.tapio.fi/schemas/forestData/operation"  
	xmlns:gml="http://www.opengis.net/gml"
	elementFormDefault="qualified"
	attributeFormDefault="qualified"
	version="V14.04">
	
	<!-- Imports / Includes -->
	<xs:import namespace="http://standardit.tapio.fi/schemas/forestData/common" schemaLocation="Common.xsd"/>
	<xs:import namespace="http://standardit.tapio.fi/schemas/virta/sampleplot" schemaLocation="VirtaSamplePlot.xsd"/>		
	<xs:import namespace="http://standardit.tapio.fi/schemas/forestData/standbase" schemaLocation="StandBase.xsd"/>
	<xs:import namespace="http://standardit.tapio.fi/schemas/forestData/specialFeatureBase" schemaLocation="SpecialFeatureBase.xsd"/>
	<xs:import namespace="http://standardit.tapio.fi/schemas/forestData/operation" schemaLocation="Operation.xsd"/>
	<xs:import namespace="http://standardit.tapio.fi/schemas/virta/sampleplot" schemaLocation="VirtaSamplePlot.xsd"/>
	<xs:import namespace="http://www.opengis.net/gml" schemaLocation="../gml/3.1.1/base/gml.xsd"/> 

	<!-- Elementtimääritykset -->
	<!-- Element definitions -->
	<xs:element name="TargetPart" type="TargetPartType"/>
	<xs:element name="Status3" type="co:ChangeStateType"/>
	<xs:element name="PartNumber" type="xs:string">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Osan numero. Virta järjestelmän antava juokseva nro. Maastotallentimella osanumerointi tehdään manuaalisesti.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>	
	</xs:element>
	<xs:element name="PartsDetectedArea" type="co:PositiveDecimalMax4IntegralPartMax2FractionalPartType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Osan havaittu määrä ha. LT = valmiissa kohteessa kenttiin on syötettävä niin paljon arvoja, että niiden summa on vähintään kuvion tai kohteen ilmoitettu määrä -kentän suuruinen.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="CuttingMethod" type="op:CuttingTypeType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Hakkuutapa.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>	
	<xs:element name="RegenerationType" type="VirtaRegenerationType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Uudistamistapa.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>	
	<xs:element name="TargetPartStatus" type="VirtaTargetPartStatusType"/>
	<xs:element name="OperationDate" type="co:DateMmDdYyyyType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Toimenpiteen päivämäärä.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>	
	<xs:element name="OperationYear" type="co:YearType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Uudistamisen tai hakkuun ajankohta.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Classification" type="co:PositiveIntegerType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Kemeravirrassa tl 30 1=taimikonh, 2=2. khlk metsänhoito, 3=verhop. poisto. Tallentimelta tulevalla uta kentän koodituksella 1-6 päivitetään kemeravirran luokitus (Lts) kenttää silloin, kun työlaji on 10-15, Uud. tapa (uta) 1 = luontainen 2 = kylvö 3 = istutus 4 = luontainen ja kylvö 5 = luontainen ja istutus 6 = kylvö ja istutus.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>	
	<xs:element name="Review" type="VirtaReviewType"/> 
	<xs:element name="Reason" type="VirtaReasonType"/>
	<xs:element name="Review2" type="VirtaReviewType"/>
	<xs:element name="Reason2" type="VirtaReasonType"/>
	<xs:element name="InspectionMethod" type="VirtaInspectionMethodType"/>
	<xs:element name="StemCount" type="co:PositiveInteger6digitsType"> <!-- TODO: Tarkista määrittely. Pitääkö tyhjä hyväksyä lukumääräkentissä? -->
		<xs:annotation>
			<xs:documentation xml:lang="fi">Runkoluku kpl/ha. Laskentasääntö: 10 000 m2*muiden kuin puuluokaltaan arvot 0, 7, 8 tai 9 omaavien puiden lkm tietojen summa/Koealojen pinta-alojen summa m2 (huomioidaan siis myös puutason lkm tiedot, joissa plk on tyhjä) Hirvivahinkolaskennoissa TIH arvo.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="SeedStemCount" type="co:PositiveInteger6digitsType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Hakatun uudistushakkuun siemenpuiden määrä kpl/ha. Laskentasääntö: 10 000 m2*Puuluokkaan 9 kuuluvien puiden lkm/Koealojen pinta-alojen summa m2 (jos hta on muu kuin 2 tai 3, niin s/k. kpl/ha kenttään Virtaan siirretään tallentimelta tämän kentän arvo).</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>	
	<xs:element name="StubStemCount" type="co:PositiveInteger6digitsType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Hakatun harvennushakkuun kantojan määrä kpl/ha. Laskentasääntö: 10 000 m2*Puuluokkaan 8 kuuluvien puiden lkm/Koealojen pinta-alojen summa m2 (jos hta on 2 tai 3, niin s/k. kpl/ha kenttään Virtaan siirretään tallentimelta tämän kentän arvo).</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="CultivatedCropStemCount" type="co:PositiveInteger6digitsType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Viljelytaimien runkoluku kpl/ha Laskentasääntö: 10 000 m2*Puuluokkaan 4 kuuluvien puiden lkm/Koealojen pinta-alojen summa m2.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>	
	<xs:element name="NaturalCropStemCount" type="co:PositiveInteger6digitsType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Kasvatuskelpoisten luonnontaimien runkoluku kpl/ha Laskentasääntö: 10 000 m2*Puuluokkaan 5 kuuluvien puiden lkm/Koealojen pinta-alojen summa m2.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>	
	<xs:element name="CompletingNaturalCropStemCount" type="co:PositiveInteger6digitsType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Täydentävien luonnontaimien runkoluku kpl/ha Laskentasääntö: 10 000 m2*Puuluokkaan 6 kuuluvien puiden lkm/Koealojen pinta-alojen summa m2.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>	
	<xs:element name="DeadStemCount" type="co:PositiveInteger6digitsType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Kuolleiden ja kelvottomien viljelytaimien runkoluku kpl/ha Laskentasääntö: 10 000 m2*Puuluokkaan 7 kuuluvien puiden lkm/Koealojen pinta-alojen summa m2.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>	
	<xs:element name="NotDamagedCount" type="co:PositiveInteger6digitsType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Niiden taimien runkoluku kpl/ha joille ei ole aiheutunut kasvua tai laatua alentavaa pysyvää vauriota. Laskentasääntö: 10 000 m2*kpl summa/Koealojen pinta-alojen summa m2.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>	
	<xs:element name="Class1damageCount" type="co:PositiveInteger6digitsType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">1. riistavaurioluokkaan kuuluvien taimien runkoluku eli rvl kentän arvo on 1. Laskentasääntö: 10 000 m2*kpl summa/Koealojen pinta-alojen summa m2 .</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>	
	<xs:element name="Class2damageCount" type="co:PositiveInteger6digitsType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">2. riistavaurioluokkaan kuuluvien taimien runkoluku eli rvl kentän arvo on 2. Laskentasääntö: 10 000 m2*kpl summa/Koealojen pinta-alojen summa m2.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>	
	<xs:element name="Class3damageCount" type="co:PositiveInteger6digitsType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">3. riistavaurioluokkaan kuuluvien taimien runkoluku eli rvl kentän arvo on 3. Laskentasääntö: 10 000 m2*kpl summa/Koealojen pinta-alojen summa m2.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>	
	<xs:element name="Class4damageCount" type="co:PositiveInteger6digitsType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">4. riistavaurioluokkaan kuuluvien taimien runkoluku eli rvl kentän arvo on 4. Laskentasääntö: 10 000 m2*kpl summa/Koealojen pinta-alojen summa m2.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>	
	<xs:element name="LargePeelDamage" type="co:PositiveInteger6digitsType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Suuren riistakuorivaurion omaavien taimien runkoluku eli rvl kentän arvo on 6. Laskentasääntö: 10 000 m2*kpl summa/Koealojen pinta-alojen summa m2.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>	
	<xs:element name="SmallPeelDamage" type="co:PositiveInteger6digitsType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Pienen riistakuorivaurion omaavien taimien runkoluku eli rvl kentän arvo on 5. Laskentasääntö: 10 000 m2*kpl summa/Koealojen pinta-alojen summa m2.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>	
	<xs:element name="DamagedSeedlingCount" type="co:PositiveInteger6digitsType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Riistavahingon vuoksi laskennallisesti kokonaan tuhoutuneiden puiden runkoluku varttuneemmissa metsissä kpl/ha.  TUH = 0,5 * A + B = X kpl/ha A=pkvrlk, B=skvrlk. Taimikoissa kun on käytetty riistavaurioluokkia 1-4 a) Kokonaan tuhoutuneiden taimien lukumäärä (TUH, kpl/ha) männylle ja muille havupuille, lukuun ottamatta kuusta ja lehtikuusta, lasketaan kaavalla: TUH = 0,15*A + 0,3*B + 0,5*C + D, missä A = taimet vahinkoluokassa I, kpl/ha B = taimet vahinkoluokassa II, kpl/ha C = taimet vahinkoluokassa III, kpl/ha D = taimet vahinkoluokassa IV, kpl/ha b) Kokonaan tuhoutuneiden taimien lukumäärä (TUH, kpl/ha) kuuselle, lehtikuuselle ja lehtipuille lasketaan kaavalla: TUH = 0,3*B + 0,5*C + D, missä B = taimet vahinkoluokassa II, kpl/ha C = taimet vahinkoluokassa III, kpl/ha D = taimet vahinkoluokassa IV, kpl/ha.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>	
	<xs:element name="Volume" type="xs:decimal"> <!-- TODO: Tarkista regular expression koodistoista, pitäiskö käyttää sitä -->
		<xs:annotation>
			<xs:documentation xml:lang="fi">Puuston tilavuus m3/ha.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>	
	<xs:element name="PineLog" type="co:PositiveInteger4digitsType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Mäntytukkitilavuus m3/ha.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>	
	<xs:element name="SpruceLog" type="co:PositiveInteger4digitsType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Kuusitukkitilavuus m3/ha.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>	
	<xs:element name="BirchLog" type="co:PositiveInteger4digitsType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Koivutukkitilavuus m3/ha.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>	
	<xs:element name="LogM3sum" type="co:PositiveInteger4digitsType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Tukkipuutavaralajin tilavuus yhteensä m3/ha. Lasketaan mänty- kuusi- ja koivukenttien summa tai syötetään arvo.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="PinePulp" type="co:PositiveInteger4digitsType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Mäntykuitutilavuus m3/ha.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="SprucePulp" type="co:PositiveInteger4digitsType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Kuusikuitutilavuus m3/ha.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>	
	<xs:element name="BirchPulp" type="co:PositiveInteger4digitsType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Koivukuitutilavuus m3/ha.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>		
	<xs:element name="PulpM3sum" type="co:PositiveInteger4digitsType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Kuitupuutavaralajin tilavuus yhteensä m3/ha. Lasketaan mänty- kuusi- ja koivukenttien summa tai syötetään arvo.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="OtherTimberValue" type="co:PositiveInteger5digitsType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Muun puutavaralajin arvo euroa/ha.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>	
	<xs:element name="EnergyTimberValue" type="co:PositiveInteger5digitsType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Energiapuun arvo euroa/ha. Lenttää ei yleensä käytetä hirvivahinkojen tarkastuksissa.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>	
	<xs:element name="CuttingValue" type="co:PositiveInteger5digitsType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">hakkuuarvo euroa/ha. Hakkuuarvoa laskettaessa käytetään Summa-arvotaulukot 2007 kirjasen s. 50 taulukkoa. ppl=1 =>mänty, ppl=2=>kuusi ja ppl on joku muu=> käytetään lehtipuun arvoa, jolla kerrotaan ytu ja yku-kenttien arvot, jotka summataan ja tulokseen lisätään mptl ja epuu kenttien summa.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>	
	<xs:element name="DestroyedCuttingValue" type="co:PositiveInteger5digitsType">	
		<xs:annotation>
			<xs:documentation xml:lang="fi">Kokonaan tuhoutuneiden puiden hakkuuarvo e/ha.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>	
	<xs:element name="ExpectedValueCoefficient" type="co:PositiveDecimalMax1IntegralPartMax2FractionalPartType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Odotusarvokerroin.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>	
	<xs:element name="ExpectedValueSurplus" type="co:PositiveDecimalMax5IntegralPartMax2FractionalPartType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Odotusarvolisä euroa/ha.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>	
	<xs:element name="Compensation" type="co:PositiveDecimalMax5IntegralPartMax2FractionalPartType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Kasvu- ja laatutappiokorvaus euroa.  Korvauksesta vähennetään taimikonhoitotyön arvo, mikäli  tata-kentän arvo on 1. Mikäli valtapituus on enintään 1,3m käytetään varhaishoidon kustannususta. Mikäli valtapituus on yli 1,3 m käytetään myöhempää kustannusta. Mikäli tata kentän arvo on 2, ei vähennystä tehdä.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>	
	<xs:element name="AlreadyPaidCompensation" type="co:PositiveDecimalMax5IntegralPartMax2FractionalPartType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Aiemmin maksettu kasvu- ja laatutappiokorvaus euroa.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>	
	<xs:element name="TotalCompensation" type="co:PositiveDecimalMax5IntegralPartMax2FractionalPartType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Korvaus yhteensä euroa.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>	
	<xs:element name="NeedForTreatment" type="co:VirtaYesNoType">	
		<xs:annotation>
			<xs:documentation xml:lang="fi">Taimikonhoitotyön tarve.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>	
	<xs:element name="Suggestion" type="VirtaSuggestionType"/>
	<xs:element name="Phase2youngCropCount" type="co:PositiveInteger5digitsType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Toisen jakson taimet kpl/ha. Lasketaan koealatason tjt kentän perusteella. Laskentasääntö: 10 000 m2*tjt kenttien arvojen summa/Koealojen pinta-alojen summa m2.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
			<xs:documentation xml:lang="en"></xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="RecommendedDensity" type="co:PositiveInteger5digitsType">
		<xs:annotation>
			<xs:documentation xml:lang="fi">Ohjetiheys kpl/ha. Määräytyy puulajin ja kasvupaikan perusteella.</xs:documentation>
			<xs:documentation xml:lang="sv"></xs:documentation>
